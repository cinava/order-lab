{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.4 || ^8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "asimlqt/php-google-spreadsheet-client": "^3.0",
        "bmatzner/fontawesome-bundle": "^4.7",
        "box/spout": "^3.1",
        "doctrine/doctrine-bundle": "2.3.2",
        "doctrine/doctrine-migrations-bundle": "^2.1",
        "doctrine/orm": "2.8.4",
        "fabiang/sasl": "^1.0",
        "friendsofsymfony/comment-bundle": "^4.0.x-dev",
        "friendsofsymfony/jsrouting-bundle": "^2.5",
        "google/apiclient": "^1.1",
        "knplabs/knp-paginator-bundle": "^5.1",
        "knplabs/knp-snappy-bundle": "^1.7",
        "knplabs/knp-time-bundle": "^1.17",
        "oneup/uploader-bundle": "^3.1",
        "paragonie/random_compat": "^2",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpoffice/phpexcel": "^1.8",
        "phpoffice/phpspreadsheet": "^1.22",
        "sensio/framework-extra-bundle": "^5.5",
        "sinergi/browser-detector": "^6.1",
        "spatie/pdf-to-text": "^1.3",
        "stof/doctrine-extensions-bundle": "^1.5",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "4.4.*",
        "symfony/console": "^4.4",
        "symfony/dotenv": "^4.4",
        "symfony/expression-language": "^4.4",
        "symfony/flex": "^1.6",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "^4.4",
        "symfony/http-client": "4.4.*",
        "symfony/intl": "4.4.*",
        "symfony/mailer": "4.4.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/process": "4.4.*",
        "symfony/property-access": "4.4.*",
        "symfony/property-info": "4.4.*",
        "symfony/proxy-manager-bridge": "4.4.*",
        "symfony/security-bundle": "4.4.*",
        "symfony/serializer": "4.4.*",
        "symfony/swiftmailer-bundle": "^3.5",
        "symfony/translation": "4.4.*",
        "symfony/twig-bundle": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/web-link": "4.4.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/workflow": "^4.4",
        "symfony/yaml": "^4.4",
        "tattali/calendar-bundle": "^1.2",
        "twig/extra-bundle": "^3.3",
        "twilio/sdk": "^6.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "4.4.*",
        "symfony/css-selector": "4.4.*",
        "symfony/debug-bundle": "^4.4",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/web-profiler-bundle": "^4.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "composer/installers": true,
            "third-party/required-plugin": true,
            "unnecessary/plugin": false,
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/"
        },
		"classmap": [
            "src/Kernel.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "pre-autoload-dump": "Google\\Task\\Composer::cleanup"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        },
        "google/apiclient-services": [
            "Drive",
            "YouTube"
        ]
    }
}
