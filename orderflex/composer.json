{
    "name": "symfony/framework-flex-edition",
    "type": "project",
    "description": "Order system",
    "license": "MIT",
    "require": {
        "php": "^7.0.8",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "adesigns/calendar-bundle": "^1.1",
        "bmatzner/fontawesome-bundle": "^4.7",
        "box/spout": "^3.1",
        "doctrine/common": "^2.11",
        "doctrine/doctrine-bundle": "^1",
        "doctrine/doctrine-fixtures-bundle": "3.3.0",
        "doctrine/doctrine-migrations-bundle": "^2.1",
        "doctrine/orm": "^2.7",
        "friendsofsymfony/comment-bundle": "^2.3",
        "friendsofsymfony/jsrouting-bundle": "^2.5",
        "friendsofsymfony/user-bundle": "^2.1",
        "google/apiclient": "1.0.*@beta",
        "asimlqt/php-google-spreadsheet-client": "^3",
        "knplabs/knp-paginator-bundle": "^4.1",
        "knplabs/knp-snappy-bundle": "^1.6",
        "oneup/uploader-bundle": "^2.1",
        "paragonie/random_compat": "^2",
        "phpoffice/phpexcel": "^1.8",
        "phpoffice/phpspreadsheet": "^1.10",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^4.0",
        "sinergi/browser-detector": "^6.1",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/assetic-bundle": "^2.8",
        "symfony/console": "3.4.*",
        "symfony/dotenv": "3.4.*",
        "symfony/expression-language": "^3.4",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "3.4.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/swiftmailer-bundle": "^2.6",
        "symfony/workflow": "3.4.*",
        "symfony/yaml": "3.4.*",
        "twig/extensions": "^1.5",
        "yzalis/crontab": "1.0.x-dev"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.1",
        "symfony/phpunit-bridge": "^5.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/"
        },
        "classmap": [
            "src/Kernel.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/App/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "3.4.*"
        },
        "src-dir": "src/App"
    }
}
