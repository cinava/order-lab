                </div>
            {% endif %}

            {{ formmacros.field(formfield.id) }}

            {% if formfield.status is defined %}
                {#{{ formmacros.field(formfield.status) }}#}
                {{ usermacros.statusVerifiedField(formfield,cicle) }}
            {% endif %}

            {{ formmacros.field(formfield.commentType) }}

            {{ formmacros.field(formfield.commentSubType) }}

            {{ formmacros.field(formfield.comment) }}

            {% if formfield.documents is defined or prototype == 'prototype' %}
                {{ usermacros.fileupload( formfield, cicle, classname, prototype ) }}
            {% endif %}

        </div>

    {% else %}

        {% do formfield.commentType.setRendered %}
        {% do formfield.commentSubType.setRendered %}
        {% do formfield.comment.setRendered %}

        {% if formfield.status is defined %}
            {% do formfield.status.setRendered %}
        {% endif %}

    {% endif %}

{% endmacro %}


{% macro snapshot( user, sitename ) %}

    {% import "OlegUserdirectoryBundle::Default/usermacros.html.twig" as usermacros %}

    {#check if assistants exist#}
    {% set assistExist = false %}
    {% if user.locations is defined %}
        {% for location in user.locations %}
            {% if location.assistant|length > 0 %}
                {% set assistExist = true %}
            {% endif %}
        {% endfor %}
    {% endif %}

    <table class="table table-condensed text-left">
    <thead>
        <tr>
            <th>Service(s)</th>
            <th>Name</th>
            <th>Administrative Title(s)</th>
            <th>Academic Title(s)</th>
            <th>Preferred Email</th>
            <th>Preferred Phone Number</th>
            <th>IC</th>
            <th>Room Number(s)</th>
            {% if assistExist %}
                <th>Assistant(s)</th>
            {% endif %}
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>
                {% for service in user.getServices %}
                    {% if service is defined and service != "" %}
                        {{ usermacros.getInstitutionalTree(service) }}
                    {% endif %}
                {% endfor %}
            </td>
            <td>{{ user.displayName }}</td>
            <td>
                {% for administrativeTitle in user.administrativeTitles %}
                    {% if administrativeTitle.name %}
                        {{ administrativeTitle.name }}<br>
                    {% endif %}
                {% endfor %}
            </td>
            <td>
                {% for appointmentTitles in user.appointmentTitles %}
                    {% if appointmentTitles.name %}
                        {{ appointmentTitles.name }}<br>
                    {% endif %}
                {% endfor %}
            </td>
            <td>
                <a href="mailto:{{ user.email }}" target="_top">{{ user.email }}</a>
            </td>
            <td>
                {% set valueclean = user.preferredPhone|replace({('+'): "", (' '): "", (')'): "", ('('): "", ('-'): ""}) %}
                <a href="tel:{{ valueclean }}" target="_top">{{ user.preferredPhone }}</a>
            </td>
            <td>
                {% for location in user.locations %}
                    {% if location.ic %}
                        {{ location.ic }}<br>
                    {% endif %}
                {% endfor %}
            </td>
            <td>
                {% for location in user.locations %}
                    {% if location.room %}
                        {{ location.room }}<br>
                    {% endif %}
                {% endfor %}
            </td>

            {% if assistExist %}
                <td>
                    {% for location in user.locations %}
                        {% if location.assistant %}
                            {% for assistant in location.assistant %}
                                <a href="{{ path(sitename~'_showuser', { 'id': assistant.id }) }}">{{ assistant.getUsernameShortest() }}</a>
                                ({{ location.name }}):
                                {% set valueclean = assistant.preferredPhone|replace({('+'): "", (' '): "", (')'): "", ('('): "", ('-'): ""}) %}
                                <a href="tel:{{ valueclean }}" target="_top">{{ assistant.preferredPhone }}</a>
                                {% if assistant.preferredPhone and assistant.email %}
                                    ,
                                {% endif %}
                                <a href="mailto:{{ assistant.email }}" target="_top">{{ assistant.email }}</a>
                                <br>
                            {% endfor %}
                        {% endif %}
                    {% endfor %}
                </td>
            {% endif %}

        </tr>
    </tbody>
    </table>

{% endmacro %}


{% macro snapshot_well( user, sitename ) %}
    {% import "OlegOrderformBundle::Default/formmacros.html.twig" as formmacros %}

    <div class="well">

        {#service tree#}
        {% set services = user.getServices() %}
        {% if services|length > 0 %}
            <p>
                <strong>Service(s)</strong>:<br>
                {% for service in services %}
                    {{ service.getParentTree() }}<br>
                {% endfor %}
            <p>
        {% endif %}

        {#administrative titles#}
        {% set titles = user.getAdministrativeTitles() %}
        {% if titles|length > 0 %}
            <p>
                <strong>Administrative Title(s)</strong>:<br>
                {% for title in titles %}
                    {{ title.name }}<br>
                {% endfor %}
            </p>
        {% endif %}

        {#appointment titles#}
        {% set titles = user.getAppointmentTitles() %}
        {% if titles|length > 0 %}
            <p>
                <strong>Academic Appointment Title(s)</strong>:<br>
                {% for title in titles %}
                    {{ title.name }}<br>
                {% endfor %}
            </p>
        {% endif %}

        {#phone#}
        <p>
            <strong>Administrative Title(s)</strong>:<br>
            {{ user.preferredPhone }}
        </p>

        {#pager#}
        {% set locations = user.locations %}
        {% if locations|length > 0 %}
            <p>
                <strong>Room(s)</strong>:<br>
                {% for location in locations %}
                    {{ location.pager }}<br>
                {% endfor %}
            </p>
        {% endif %}

        {#room#}
        {% set locations = user.locations %}
        {% if locations|length > 0 %}
            <p>
                <strong>Room(s)</strong>:<br>
                {% for location in locations %}
                    {{ location.room }}<br>
                {% endfor %}
            </p>
        {% endif %}

        {#assistance#}
        {% set locations = user.locations %}
        {% if locations|length > 0 %}
            <p>
                <strong>Assistant(s)</strong>:<br>
                {% for location in locations %}
                    {% for assistant in location.assistant %}
                        {{ assistant.getUserNameStr() }}<br>
                    {% endfor %}
                {% endfor %}
            </p>
        {% endif %}

    </div>

{% endmacro %}



{% macro roleAttributes( roleobjects ) %}
