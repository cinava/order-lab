{% import "OlegOrderformBundle::Default/formmacros.html.twig" as formmacros %}
{% import "OlegUserdirectoryBundle::Tree/treemacros.html.twig" as treemacros %}

<div class="container text-center">

    <input type="hidden" id="baseurl" value="{{app.request.host}}{{app.request.getBaseURL()}}" />
    <input type="hidden" id="formcycle" value="accountreq" />
    <input type="hidden" id="orderformtype" value="single" />

    <h2 class="alert alert-info well-lg safary-fix" align="center">{{ title }}</h2>

    <br>

    {#<form id="accountrequestform" action="{{ path(sitename~'_accountrequest_create') }}" method="post">#}
    {{ form_start(form) }}
    {{ form_errors(form) }}

    {{ formmacros.field(form.userName) }}

    {#{{ form_errors(form.hashPassword) }}#}
    {#{{ formmacros.field(form.hashPassword.first) }}#}
    {#{{ formmacros.field(form.hashPassword.second) }}#}
    {{ formmacros.field(form.hashPassword) }}

    {{ formmacros.field(form.email) }}

    {% if form.submit is defined %}
        <p>
            {{ form_widget(form.submit) }}
        </p>
    {% endif %}

    {{ form_end(form) }}

    {% include 'OlegUserdirectoryBundle::Default/footer.html.twig' %}

</div> <!-- /container -->

