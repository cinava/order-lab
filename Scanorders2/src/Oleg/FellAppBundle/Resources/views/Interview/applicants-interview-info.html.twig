{#{% extends "OlegFellAppBundle::Default/base.html.twig" %}#}

{% block header %}
{% endblock %}


{% import "OlegFellAppBundle::Default/fellappmacros.html.twig" as fellappmacros %}


{% block content %}

{% if withIconHref is not defined %}
    {% set withIconHref = true %}
{% endif %}

{% for entity in entities %}

    <div class="text-left col-xs-12">

        {#page-break-after: always;#}
        <div class="pdf-page">

            <h4>
            {{ fellappmacros.applicantInterviewsInfo(entity,withIconHref) }}
            </h4>

            {{ fellappmacros.interviewsDetails(entity,withIconHref) }}

        </div>

    </div>

    <p><!-- pagebreak --></p>
    <div style="page-break-before:always">&nbsp;</div>

{% endfor %}


{% endblock %}
