{#{% extends "OlegFellAppBundle::Default/base.html.twig" %}#}

{% stylesheets
'bundles/oleguserdirectory/bootstrap/css/*' filter='cssrewrite'
'bundles/oleguserdirectory/form/css/form.css' filter='cssrewrite'
%}
<link rel="stylesheet" href="{{ asset_url }}" />
{% endstylesheets %}

{% block header %}{% endblock %}


{% import "OlegFellAppBundle::Default/fellappmacros.html.twig" as fellappmacros %}


{% block content %}


{% for entity in entities %}

    <div style="page-break-before:always">&nbsp;</div>

    <div class="text-left col-xs-12">

        <h4>
        {{ fellappmacros.applicantInterviewsInfo(entity) }}
        </h4>

        {{ fellappmacros.interviewsDetails(entity,true) }}

    </div>

{% endfor %}


{% endblock %}

{% javascripts
'@OlegUserdirectoryBundle/Resources/public/jquery/jquery-1.11.0.min.js'
'@OlegUserdirectoryBundle/Resources/public/jquery-ui-1.11.2/jquery-ui.js'
'@OlegUserdirectoryBundle/Resources/public/bootstrap/js/*'
%}
<script type="text/javascript" src="{{ asset_url }}"></script>
{% endjavascripts %}

