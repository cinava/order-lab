

    <h3 class="text-info">Research Objects</h3>


    <table class="records_list table table-hover table-condensed text-left">
        <thead>
        <tr>
            <th>Id</th>
            <th>Course</th>
            <th>Lesson</th>
            <th>Principal</th>
            <th>Principal as eneterd by user</th>
            <th>Order</th>
            <th>Actions</th>
        </tr>
        </thead>
        <tbody data-link="row" class="rowlink">
        {% for research in researches %}

            <tr>
                <td><a href="{{ path('research_edit', { 'id': research.id }) }}">{{ research.id }}</a></td>

                <td>{{ research.project }}</td>

                <td>{{ research.settitle }}</td>

                <td>{{ research.principalstr }}</td>

                <td>{{ research.principal }}</td>

                <td>
                    {#{{ research.orderinfo.id }}#}
                </td>


                <td class="rowlink-skip">

                    <div class="btn-group">
                        <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown">
                            Action <span class="caret"></span>
                        </button>
                        <ul class="dropdown-menu dropdown-menu-right">

                            <li><a data-confirm="Are you sure you want to Decline"
                                   href="{{ path('research_edit', { 'id': research.id }) }}">Review</a>
                            </li>

                            {#<li><a data-confirm="Are you sure you want to Revert Status to Active"#}
                                   {#href="{{ path('accountrequest_status', { 'id': research.orderinfo.id, 'status': 'active' }) }}">View Order</a>#}
                            {#</li>#}

                        </ul>
                    </div>

                </td>

            </tr>
        {% endfor %}
        </tbody>
    </table>
